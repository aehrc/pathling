      "remarks": [
        "======================================================================",
        " FEMALE SEXUAL ACTIVITY                                               ",
        "======================================================================",
        "Sexual activity is modeled on a yearly basis, matching the Indiana University ",
        "study. The incidences used here match the 'Vaginal Intercourse' numbers from ",
        "the study."
      ],
      "exact": {
        "quantity": 1,
        "unit": "years"
      },
      "complex_transition": [
        {
          "condition": {
            "condition_type": "Age",
            "operator": "<",
            "quantity": 16,
            "unit": "years"
          },
          "distributions": [
