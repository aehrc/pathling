{
  "title": "Type hinting with tags",
  "resources": [
    {
      "resourceType": "Patient",
      "id": "123",
      "active": true,
      "name": [
        {
          "family": "Smith",
          "given": ["John"]
        }
      ]
    },
    {
      "resourceType": "Patient",
      "id": "p2",
      "active": false,
      "name": [
        {
          "family": "Doe",
          "given": ["Jane"]
        }
      ]
    }
  ],
  "tests": [
    {
      "title": "Patient status with ANSI type hint",
      "view": {
        "resource": "Patient",
        "constant": [
          {
            "name": "decimalValue",
            "valueDecimal": 1232.33
          }
        ],
        "select": [
          {
            "column": [
              {
                "name": "id",
                "path": "id",
                "tag": [
                  {
                    "name": "ansi/type",
                    "value": "INT"
                  }
                ]
              },
              {
                "name": "status",
                "path": "active",
                "tag": [
                  {
                    "name": "ansi/type",
                    "value": "VARCHAR(10)"
                  }
                ]
              },
              {
                "name": "decimalValue",
                "path": "%decimalValue"
              }
            ]
          }
        ]
      },
      "expect": [
        {
          "id": 123,
          "status": "true",
          "decimalValue": "1232.33"
        },
        {
          "id": null,
          "status": "false",
          "decimalValue": "1232.33"
        }
      ]
    }
  ]
}
