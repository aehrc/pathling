{
  "resourceType": "ViewDefinition",
  "id": "PatientAddresses",
  "select": [
    {
      "column": [
        {
          "path": "getResourceKey()",
          "name": "patient_id"
        }
      ]
    },
    {
      "column": [
        {
          "path": "line.join('\n')",
          "name": "street",
          "description": "The full street address, including newlines if present."
        },
        {
          "path": "use",
          "name": "use"
        },
        {
          "path": "city",
          "name": "city"
        },
        {
          "path": "postalCode",
          "name": "zip"
        }
      ],
      "forEach": "address"
    }
  ],
  "name": "patient_addresses",
  "status": "draft",
  "resource": "Patient"
}
